{
  "name": "@paper/backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js --external:@prisma/client --external:prisma",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@google-cloud/storage": "^7.16.0",
    "@paper/shared": "workspace:*",
    "@prisma/client": "^5.8.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/multer": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "multer": "^2.0.2",
    "openai": "^5.10.1",
    "sharp": "^0.33.0",
    "speakeasy": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.8",
    "@types/speakeasy": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "prisma": "^5.8.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}